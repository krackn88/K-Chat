{
  "name": "k-chat",
  "version": "0.1.0",
  "description": "An open-source self-hosted messaging platform with AI integration",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "client": "cd client && npm start",
    "dev:full": "concurrently \"npm run dev\" \"npm run client\"",
    "install:all": "npm install && cd client && npm install",
    "build": "cd client && npm run build",
    "test": "jest"
  },
  "keywords": [
    "chat",
    "messaging",
    "telegram",
    "self-hosted",
    "open-source",
    "ai"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.1",
    "multer": "^1.4.5-lts.1",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1"
  }
}
